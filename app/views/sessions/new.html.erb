Log in page

<%= form_for :session, url: login_path do |f| %>
  <%= f.label 'Email:' %><br>
  <%= f.email_field :email %><br>
  <%= f.label 'Password:' %>
  <%= link_to '(forgot password?)', new_password_reset_path %><br>
  <%= f.password_field :password %><br>
  <%= f.check_box :remember_me %>
  <%= f.label 'remember me' %><br>
  <%= f.submit 'Log in' %>
<% end %>