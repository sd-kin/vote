<% if logged_in? %>
  Rating: 
  <%= button_to '+', increase_rating_path(@rating), remote: true, disabled: @rating.increased_by?(current_user) %>
  <%= @rating.value %> <br>
  <%= button_to '-', decrease_rating_path(@rating), remote: true, disabled: @rating.decreased_by?(current_user) %>
<% end %>
